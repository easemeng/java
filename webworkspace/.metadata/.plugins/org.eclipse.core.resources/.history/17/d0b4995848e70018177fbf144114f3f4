package com.igeek.enumtest;

public class PersonSex {

	private final String sex;
	private final String sexdesc;
	
	private PersonSex(String sex,String sexdesc){
		
		this.sex = sex;
		this.sexdesc = sexdesc;
		
	}

	public String getSex() {
		return sex;
	}

	public String getSexdesc() {
		return sexdesc;
	}

	public static PersonSex MAN = new PersonSex("男","男神");
	public static final PersonSex WOMAN = new PersonSex("女","女神");
	public static final PersonSex SEMON = new PersonSex("妖","妖人");
	
	@Override
	public String toString() {
		return "Sex [sex=" + sex + ", sexdesc=" + sexdesc + "]";
	}

	public void behavior(){
		
		System.out.println("睡觉");
		
	}
}
