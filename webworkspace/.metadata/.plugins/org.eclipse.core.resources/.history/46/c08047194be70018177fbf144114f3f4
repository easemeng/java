package com.igeek.enumtest;

public enum PersonSex3 {
	
	private final String sex;
	private final String sexdesc;
	
	private PersonSex3(String sex,String sexdesc){
		
		this.sex = sex;
		this.sexdesc = sexdesc;
		
	}

	public String getSex() {
		return sex;
	}

	public String getSexdesc() {
		return sexdesc;
	}

	public static final PersonSex3 MAN = new PersonSex3("男","男神");
	public static final PersonSex3 WOMAN = new PersonSex3("女","女神");
	public static final PersonSex3 SEMON = new PersonSex3("妖","妖人");
	
	@Override
	public String toString() {
		return "Sex [sex=" + sex + ", sexdesc=" + sexdesc + "]";
	}

	public void behavior(){
		
		System.out.println("睡觉");
		
	}
}
