package com.igeek.singleton;

// 单例模式――饿汉式
public class Singleton1 {
	
	private Singleton1(){};
	
	private static Singleton1 singleton = new Singleton1();
	
	public static Singleton1 getSingleton(){
		
		return singleton;
		
	}
	
}

class Singleton11{
	
	private Singleton1 singleton;
	static{
		
	}
	
}
