package com.igeek.day05.collection;

import java.util.ArrayList;

import com.igeek.day05.objarr.Phone;

//collection 集合框架 。
public class ArrayListTest {

	public static void main(String[] args) {

		//集合 。
		ArrayList<Object> list = new ArrayList<Object>();
		
		list.add("hello");
		list.add(123);
		list.add(new Phone());
		list.add(null);
		list.add("sadfs");
		System.out.println(list.size());
		
		for (int i = 0; i < list.size(); i++) {
			System.out.println(list.get(i));
		}
		
		//size() 表示集合的大小 。
		//add() 向容器中添加元素。
		//get(int index) 获取指定下标所对应的元素。
		
		
		
		
		//collection 集合框架 。
		//集合 。
		ArrayList<Object> list2 = new ArrayList<Object>();
		
		list2.add("hello");
		list2.add(123);
		list2.add(new Phone());
		list2.add(null);
		list2.add("sadfs");
		System.out.println(list2.size());
		
		for (int i = 0; i < list2.size(); i++) {
			System.out.println(list2.get(i));
		}
		
		//size() 表示集合的大小 。
		//add() 向容器中添加元素。
		//get(int index) 获取指定下标所对应的元素。
		
		
		
		
		//collection 集合框架 。
		//集合 。
		ArrayList<Object> list3 = new ArrayList<Object>();
		
		list3.add("hello");
		list3.add(123);
		list3.add(new Phone());
		list3.add(null);
		list3.add("sadfs");
		System.out.println(list3.size());
		
		for (int i = 0; i < list3.size(); i++) {
			System.out.println(list3.get(i));
		}
		
		//size() 表示集合的大小 。
		//add() 向容器中添加元素。
		//get(int index) 获取指定下标所对应的元素。
		
		
		
		
		//collection 集合框架 。
		//集合 。
		ArrayList<Object> list4 = new ArrayList<Object>();
		
		list4.add("hello");
		list4.add(123);
		list4.add(new Phone());
		list4.add(null);
		list4.add("sadfs");
		System.out.println(list4.size());
		
		for (int i = 0; i < list4.size(); i++) {
			System.out.println(list4.get(i));
		}
		
		//size() 表示集合的大小 。
		//add() 向容器中添加元素。
		//get(int index) 获取指定下标所对应的元素。
		
		
		
		
		//collection 集合框架 。
		//集合 。
		ArrayList<Object> list5 = new ArrayList<Object>();
		
		list5.add("hello");
		list5.add(123);
		list5.add(new Phone());
		list5.add(null);
		list5.add("sadfs");
		System.out.println(list5.size());
		
		for (int i = 0; i < list5.size(); i++) {
			System.out.println(list5.get(i));
		}
		
		//size() 表示集合的大小 。
		//add() 向容器中添加元素。
		//get(int index) 获取指定下标所对应的元素。
		
	}

}
