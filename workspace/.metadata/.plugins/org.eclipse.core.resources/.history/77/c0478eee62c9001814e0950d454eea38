package com.igeek.day05.objarr;

public class ObjectArrays {

	public static void main(String[] args) {
  
		//对象数组 ？？？
		int [] i = new int []{1,23,43,53,2};
		
		String [] strs = new String [] {"abc","haha","hehe"};
		
	
		Phone [] phones = new Phone[]{new Phone(),new Phone()};
		
		Animal [] animals = new Animal[]{new Animal()};
		
		User [] users = {new User("admin","sbsbsb123","戢浪"),new User("wxwsb123","123123","吴鑫文"),new User("zpl","321","赵英俊")};
		
		//容器 。   
		
		
		//1.数组一旦声明长度不可改变。  2.数据类型是死的。
		
	}

}
