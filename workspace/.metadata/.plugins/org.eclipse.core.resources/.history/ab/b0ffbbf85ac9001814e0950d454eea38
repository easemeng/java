public class ArgumentsDemo1 {

	public static void main(String[] args) {
		int a = 10;
		int b = 20;//å±?éƒ¨å˜é‡åœ¨æ ˆä¸­ã€?
		change(a,b);
	}
	
	public static void change(int a , int b){
		a = 100;
		b = 200;
		System.out.println(a);
		System.out.println(b);
	}

}
