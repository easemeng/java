package com.igeek.day08.exception1;

public class TestException {

	@SuppressWarnings("unused")
	public static void main(String[] args) {
		// 编译时异常 。 
		// 运行时异常 。  java.long.ArithmeticException
		// try 捕获异常  catch 处理异常 。 
		// 在try 中放的是可能出现异常的代码
		// 一旦加入了catch之后 ， 当try 中的代码出现异常 。 就会执行catch中的代码 。
		
		try {
			int a = 10;
			System.out.println(100/10);
		} catch (Exception e) {
			// e.printStackTrace();
			System.out.println("除数不能为0");
			// e.printStackTrace();  打印异常信息
			// e.getM
		}finally{
			System.out.println("无论是否产生异常，都会执行finally中的代码");
		}
		
		// final 跟  finally 的区别

	}

}
